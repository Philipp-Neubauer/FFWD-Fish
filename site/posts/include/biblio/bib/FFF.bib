	author = {Blanchard, Julia L. and Jennings, Simon and Holmes, Robert and Harle, James and Merino, Gorka and Allen, J. Icarus and Holt, Jason and Dulvy, Nicholas K. and Barange, Manuel},
	month = nov,
	year = {2012},
	pmid = {23007086},
	keywords = {benthic–pelagic coupling, fisheries ecology, global environmental change, marine communities, marine macroecology, size spectrum},
	pages = {2979--2989},
	file = {Blanchard et al. - 2012 - Potential consequences of climate change for prima.pdf:/Users/phil/Dropbox/biblio/zotero/storage/J9MSTZXC/Blanchard et al. - 2012 - Potential consequences of climate change for prima.pdf:application/pdf;Data Supplement:/Users/phil/Dropbox/biblio/zotero/storage/B9ES3284/rstb.2012.0231.DC1.html:text/html;rstb20120231supp1.docx:/Users/phil/Dropbox/biblio/zotero/storage/IXWI8W72/rstb20120231supp1.docx:application/vnd.openxmlformats-officedocument.wordprocessingml.document;Snapshot:/Users/phil/Dropbox/biblio/zotero/storage/EXDAWPGR/2979.html:text/html}
