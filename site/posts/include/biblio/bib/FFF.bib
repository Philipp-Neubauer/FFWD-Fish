	pmcid = {PMC3167536},
	keywords = {Animals, Aquatic Organisms, Biodiversity, Biomass, Carbon Dioxide, ecosystem, Environment, Hydrogen-Ion Concentration, Oceans and Seas},
	pages = {14515--14520}
}

